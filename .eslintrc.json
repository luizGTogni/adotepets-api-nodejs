{
  "extends": [
    "@rocketseat/eslint-config/node"
  ],
  "rules": {
    "indent": 0,
    "camelcase": "off",
    "no-useless-constructor": "off",
    "@typescript-eslint/no-explicit-any": "off",
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 80,
        "tabWidth": 2,
        "useTabs": false,
        "semi": true,
        "endOfLine": "auto"
      }
    ]
  }
}